{
  "name": "nft-marketplace",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"cd server && npm start\" \"npm run update-readme-watch\"",
    "dev:frontend": "vite",
    "dev:server": "cd server && npm start",
    "build": "npx hardhat compile && vite build",
    "preview": "vite preview",
    "update-readme-stats": "node scripts/updateReadmeStats.js",
    "update-readme-watch": "nodemon -w server/nft-stats.json --exec \"npm run update-readme-stats\"",
    "install-git-hooks": "node scripts/installGitHooks.js",
    "postinstall": "test -d .git && npm run install-git-hooks || echo 'Skipping git hooks installation'"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "chai": "^4.5.0",
    "concurrently": "^9.2.1",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.19.4",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.6",
    "@vitejs/plugin-react": "^5.0.2",
    "axios": "^1.12.2",
    "ethers": "^5.8.0",
    "form-data": "^4.0.4",
    "lucide-react": "^0.542.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "vite": "^7.1.4"
  }
}
